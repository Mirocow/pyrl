# Example 9525 - BFS simulation
def bfs($start, %graph):
    @visited = []
    @queue = [$start]
    while len(@queue) > 0:
        $node = dequeue()
        if $node not in @visited:
            @visited = append(@visited, $node)
            for $neighbor in %graph[$node]:
                if $neighbor not in @visited:
                    enqueue($neighbor)
    return @visited

# Example 9526 - DFS simulation
def dfs($node, %graph, @visited):
    if $node in @visited:
        return @visited
    @visited = append(@visited, $node)
    for $neighbor in %graph[$node]:
        @visited = dfs($neighbor, %graph, @visited)
    return @visited

