# Pyrl Configuration File
# Copy this file to .env and modify as needed

# Data directory for Pyrl files
PYRL_DATA_DIR=/home/z/pyrl/data

# Models directory for trained models
PYRL_MODELS_DIR=/home/z/pyrl/models

# Examples directory for code examples
PYRL_EXAMPLES_DIR=/home/z/pyrl/examples

# Logs directory
PYRL_LOGS_DIR=/home/z/pyrl/logs

# Cache directory
PYRL_CACHE_DIR=/home/z/pyrl/cache

# Debug mode (true/false)
PYRL_DEBUG=false

# Log level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
PYRL_LOG_LEVEL=INFO

# Server configuration
PYRL_SERVER_HOST=0.0.0.0
PYRL_SERVER_PORT=8000

# Maximum number of examples to generate
PYRL_MAX_EXAMPLES=10000
