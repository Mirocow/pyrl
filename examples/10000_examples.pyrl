# Example 718 - Decorator-like pattern
def log_call($func):
    def wrapper(*$args):
        print("Calling function...")
        return $func(*$args)
    return wrapper
def add($a, $b):
    return $a + $b
$logged_add = log_call(add)
print($logged_add(3, 5))

# Example 719 - Memoization pattern
%cache = {}
def fib_memo($n):
    if $n in %cache:
        return %cache[$n]
    if $n <= 1:
        return $n
    $result = fib_memo($n - 1) + fib_memo($n - 2)
    %cache[$n] = $result
    return $result
print(fib_memo(20))

# Example 720 - Generator-like pattern
def range_gen($start, $end):
    @result = []
    $i = $start
    while $i < $end:
        @result = append(@result, $i)
        $i = $i + 1
    return @result
for $i in range_gen(0, 5):
    print($i)

