# Example 9523 - Tree traversal (in-order simulation)
def inorder_traversal($node):
    if $node is None:
        return []
    @result = []
    # left
    if $node.left is not None:
        @result = extend(@result, inorder_traversal($node.left))
    # root
    @result = append(@result, $node.value)
    # right
    if $node.right is not None:
        @result = extend(@result, inorder_traversal($node.right))
    return @result

